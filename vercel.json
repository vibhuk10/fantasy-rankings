{
    "version": 2,
    "builds": [
        {
            "src": "frontend/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/hello",
            "dest": "/api/hello.js"
        },
        {
            "src": "/api/qb-simple",
            "dest": "/api/qb-simple.js"
        },
        {
            "src": "/api/qb",
            "dest": "/api/qb.js"
        },
        {
            "src": "/api/rb",
            "dest": "/api/rb.js"
        },
        {
            "src": "/api/wr",
            "dest": "/api/wr.js"
        },
        {
            "src": "/api/te",
            "dest": "/api/te.js"
        },
        {
            "src": "/api/adp/qb",
            "dest": "/api/adp/qb.js"
        },
        {
            "src": "/api/adp/rb",
            "dest": "/api/adp/rb.js"
        },
        {
            "src": "/api/adp/wr",
            "dest": "/api/adp/wr.js"
        },
        {
            "src": "/api/adp/te",
            "dest": "/api/adp/te.js"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        }
    ]
}